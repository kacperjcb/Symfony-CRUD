{% extends 'base.html.twig' %}
{% block body %}
    {% if (is_granted('ROLE_ADMIN'))%}
    <table class="table table-striped table-dark">
        <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Surname</th>
            <th>City</th>
            <th>Kod_pocztowy</th>
            <th>Address</th>
            <th>Product_Name</th>
            <th>Description</th>
        </tr>
        </thead>
        <tbody>

{% for crud in polaczone %}






    <tr>
        <td>{{crud.id }}</td>
        <td>{{crud.Name }}</td>
        <td> {{crud.Surname}}</td>
        <td> {{crud.City}}</td>
        <td> {{crud.kod_pocztowy}}</td>
        <td> {{crud.Address}}</td>
        <td> {{crud.Product_Name}}</td>
        <td> {{crud.Description}}</td>
    </tr>
{% else %}
    <tr>
        <td colspan="6">no records found</td>
    </tr>
{% endfor %}
</tbody>
</table>

 {% endif %}
{% endblock %}